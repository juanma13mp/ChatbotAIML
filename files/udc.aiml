  <category>
      <pattern>HOLA</pattern>
      <template>Hola, buenos dias ¿Necesitas informacion sobre alguno de nuestros productos?</template>
  </category>
  
    <category>
      <pattern>HOLA BUENOS DIAS</pattern>
      <template><srai>HOLA</srai></template>
  </category>
  
    <category>
      <pattern>BUENOS DIAS</pattern>
      <template><srai>HOLA</srai></template>
  </category>
  
  <category>
      <pattern>SI</pattern>
      <that>Hola, buenos dias ¿Necesitas informacion sobre alguno de nuestros productos</that>
